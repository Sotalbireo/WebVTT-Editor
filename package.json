{
  "name": "WebVTT-Editor",
  "version": "0.1.0",
  "description": "The editor to create WebVTT how subtitles at html5 video.",
  "keywords": [
    "html5",
    "WebVTT",
    "subtitles",
    "electron"
  ],
  "author": "Sota SASAKI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/sotalbireo/webvtt-editor.git"
  },
  "bugs": "http://github.com/sotalbireo/webvtt-editor/issues",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm-run-all cp:html build:* electron",
    "electron": "npx electron .",
    "cp:html": "cpx src/index.html dist",
    "build": "npm-run-all build:*",
    "build:m": "webpack --config webpack.config.main.js --colors",
    "build:r": "webpack --config webpack.config.renderer.js --colors",
    "watch": "npm-run-all --parallel watch:* watch:cp:*",
    "watch:m": "webpack --config webpack.config.main.js --colors --watch",
    "watch:r": "webpack --config webpack.config.renderer.js --colors --watch",
    "watch:cp:html": "cpx src/index.html dist --watch",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^8.0.58",
    "@types/react": "^16.0.29",
    "@types/react-dom": "^16.0.2",
    "awesome-typescript-loader": "^3.4.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.7",
    "electron-packager": "^10.1.0",
    "file-loader": "^1.1.5",
    "npm-run-all": "^4.1.2",
    "semantic-ui-react": "^0.76.0",
    "style-loader": "^0.19.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "electron": "^1.7.0",
    "jschardet": "^1.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.0"
  }
}
