{
  "name": "webpack-editor",
  "version": "0.2.0",
  "description": "The editor to create WebVTT how subtitles at html5 video.",
  "keywords": [
    "html5",
    "WebVTT",
    "subtitles",
    "electron"
  ],
  "author": "Kyohei SASAKI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/sotalbireo/webvtt-editor.git"
  },
  "bugs": "http://github.com/sotalbireo/webvtt-editor/issues",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm-run-all --parallel cp:html build:* --sequential electron",
    "electron": "npx electron .",
    "cp:html": "cpx src/index.html dist",
    "build": "npm-run-all build:*",
    "build:m": "webpack --run-dev --config webpack.main.js",
    "build:r": "webpack --run-dev --config webpack.renderer.js",
    "watch": "npm-run-all --parallel watch:* watch:cp:*",
    "watch:m": "webpack --config webpack.config.main.js --colors --watch",
    "watch:r": "webpack --config webpack.config.renderer.js --colors --watch",
    "watch:cp:html": "cpx src/index.html dist --watch",
    "-postinstall": "npm-run-all --parallel postinstall:*",
    "postinstall:": ""
  },
  "devDependencies": {
    "@types/node": "^8.10.18",
    "@types/webpack": "^4.1.5",
    "awesome-typescript-loader": "^5.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "electron-packager": "^12.1.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-command": "^0.2.0"
  },
  "dependencies": {
    "electron": "^2.0.2",
    "jschardet": "^1.6.0"
  }
}
